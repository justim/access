{
    "name": "justim/access",
    "description": "A simple MySQL wrapper optimized for bigger data sets",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Access\\": "src"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.2.0",
        "phpunit/phpunit": "^9.5.11",
        "php-coveralls/php-coveralls": "^2.5.2",
        "phpstan/phpstan-phpunit": "^1.0.0",
        "squizlabs/php_codesniffer": "^3.6.2",
        "phpbench/phpbench": "^1.2.3",
        "vimeo/psalm": "^4.16.1",
        "symfony/var-dumper": "^6.0.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Benchmarks\\": "benchmarks",
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "XDEBUG_MODE=coverage phpunit",
        "bench": "phpbench run --progress=blinken --report=aggregate",
        "lint": "phpstan analyse -c phpstan.neon && psalm --show-info=false && phpcs -p"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
